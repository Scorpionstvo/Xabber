<?xml version="1.0" encoding="utf-8"?>
<level-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Regular chat -->
    <item android:minLevel="0" android:maxLevel="0" android:drawable="@drawable/ic_status_chat"/>
    <item android:minLevel="1" android:maxLevel="1" android:drawable="@drawable/ic_status_online"/>
    <item android:minLevel="2" android:maxLevel="2" android:drawable="@drawable/ic_status_away"/>
    <item android:minLevel="3" android:maxLevel="3" android:drawable="@drawable/ic_status_xa"/>
    <item android:minLevel="4" android:maxLevel="4" android:drawable="@drawable/ic_status_dnd"/>


    <!-- Public group -->
    <item android:minLevel="20" android:maxLevel="20" android:drawable="@drawable/ic_status_public_group_chat"/>


    <!-- Incognito group -->
    <item android:minLevel="30" android:maxLevel="30" android:drawable="@drawable/ic_status_incognito_group_chat"/>
    <item android:minLevel="31" android:maxLevel="31" android:drawable="@drawable/ic_status_incognito_group_online"/>
    <item android:minLevel="32" android:maxLevel="32" android:drawable="@drawable/ic_status_incognito_group_away"/>
    <item android:minLevel="33" android:maxLevel="33" android:drawable="@drawable/ic_status_incognito_group_xa"/>
    <item android:minLevel="34" android:maxLevel="34" android:drawable="@drawable/ic_status_incognito_group_dnd"/>
    <item android:minLevel="35" android:maxLevel="35" android:drawable="@drawable/ic_status_incognito_group_unavailable"/>


    <!-- Bot -->
    <item android:minLevel="60" android:maxLevel="60" android:drawable="@drawable/ic_status_bot_chat"/>
    <item android:minLevel="61" android:maxLevel="61" android:drawable="@drawable/ic_status_bot_online"/>
    <item android:minLevel="62" android:maxLevel="62" android:drawable="@drawable/ic_status_bot_away"/>
    <item android:minLevel="63" android:maxLevel="63" android:drawable="@drawable/ic_status_bot_xa"/>
    <item android:minLevel="64" android:maxLevel="64" android:drawable="@drawable/ic_status_bot_dnd"/>
    <item android:minLevel="65" android:maxLevel="65" android:drawable="@drawable/ic_status_bot_unavailable"/>



</level-list>
