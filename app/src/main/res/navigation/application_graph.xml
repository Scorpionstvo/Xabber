<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/chatFragment">

    <fragment
        android:id="@+id/chatFragment"
        android:name="com.xabber.presentation.application.fragments.chat.ChatFragment"
        android:label="Xabber"
        tools:layout="R.layout.fragment_chat">
        <action
            android:id="@+id/action_chatFragment_to_onBoardingActivity"
            app:destination="@id/onBoardingActivity" />
    </fragment>
    <activity
        android:id="@+id/onBoardingActivity"
        android:name="com.xabber.presentation.onboarding.activity.OnBoardingActivity"
        android:label="activity_onboarding"
        tools:layout="@layout/activity_onboarding" />
</navigation>